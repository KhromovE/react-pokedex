{
  "name": "pokedex",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://khromove.github.io/react-pokedex/",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "mobx": "^5.9.0",
    "mobx-react": "^5.4.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-paginate": "^6.2.1",
    "react-scripts": "2.1.3",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "extends": [
      "standard",
      "plugin:react/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "standard",
      "react",
      "jsx-a11y",
      "import",
      "prettier"
    ],
    "globals": {
      "fetch": false,
      "it": false,
      "describe": false,
      "expect": false,
      "beforeEach": false
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "trailingComma": "all",
          "singleQuote": true,
          "printWidth": 120,
          "semi": false
        }
      ]
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "eslint-config-prettier": "^4.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "gh-pages": "^2.0.1",
    "jest-fetch-mock": "^2.1.0",
    "prettier": "^1.16.3"
  }
}